{
  "name": "blog-api-nodejs",
  "description": "blog platform, restful api, nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest NODE_ENV= test",
    "start": "node dist/index.js NODE_ENV=production",
    "dev": "nodemon --exec babel-node src/index.js NODE_ENV=development",
    "build": "babel src -d dist",
    "migrate": "sequelize db:migrate --env development",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matabaroAthy/blog-api-nodejs.git"
  },
  "keywords": [
    "blog",
    "platform",
    "restful",
    "api",
    "nodejs"
  ],
  "author": "Athanase Matabaro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matabaroAthy/blog-api-nodejs/issues"
  },
  "homepage": "https://github.com/matabaroAthy/blog-api-nodejs#readme",
  "dependencies": {
    "@babel/node": "^7.13.10",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "*",
    "jest": "^26.6.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
